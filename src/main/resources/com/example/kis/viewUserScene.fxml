<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            style="-fx-background-color: #ada6f5;"
            xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.kis.ViewUserController">
    <children>
        <AnchorPane layoutY="136.0" prefHeight="266.0" prefWidth="603.0" style="-fx-background-color: edeff0;">
            <children>
                <TableView fx:id="tableResources" layoutY="2.0" prefHeight="266.0" prefWidth="550.0">
                    <columns>
                        <TableColumn fx:id="col_id" prefWidth="50.0" text="id" />
                        <TableColumn fx:id="col_name" prefWidth="150.0" text="name" />
                        <TableColumn fx:id="col_login" prefWidth="150.0" text="login" />
                        <TableColumn fx:id="col_password" prefWidth="150.0" text="password" />
                        <TableColumn fx:id="col_law" prefWidth="50.0" text="law_id" />
                    </columns>
                </TableView>
                <Button fx:id="btn_update" layoutX="549.0" layoutY="2.0" maxHeight="40.0" maxWidth="53.0" minHeight="19.0" minWidth="53.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="53.0" text="UpD" />
                <Button fx:id="btn_delete" layoutX="549.0" layoutY="52.0" maxHeight="40.0" maxWidth="53.0" minHeight="19.0" minWidth="53.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="53.0" text="Del" />
            </children>
        </AnchorPane>
        <Label fx:id="username_label" layoutX="336.0" layoutY="22.0" prefHeight="30.0" prefWidth="171.0" style="-fx-background-color: fcf9a2;" textAlignment="RIGHT">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="btn_insert" layoutX="501.0" layoutY="100.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: fcdf95;" text="Добавить">
            <font>
                <Font size="14.0" />
            </font></Button>
        <Label layoutX="14.0" layoutY="100.0" prefHeight="30.0" prefWidth="171.0" text="Таблица: Users">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="btn_exit" layoutX="514.0" layoutY="22.0" minHeight="21.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="74.0" text="Выход" />
        <Label layoutX="17.0" layoutY="23.0" text="Режим администратора">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
      <Button fx:id="btn_returnBack" layoutX="14.0" layoutY="59.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="197.0" style="-fx-background-color: fff;" text="Вернуться к таблице Product" />
    </children>
</AnchorPane>
